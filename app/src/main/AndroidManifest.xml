<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ck.no.mind">

    <application

        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mind">
        <activity android:name=".activities.PsycologistsActivity"></activity>
        <activity android:name=".activities.EventActivity" />
        <activity android:name=".activities.HeartMonitorActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />
        <uses-library android:name="org.apache.http.legacy" android:required="false" />

    </application>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="aandroid.permission.INTERNET" />

    <uses-permission android:name="android.permission.BODY_SENSORS" />

</manifest>